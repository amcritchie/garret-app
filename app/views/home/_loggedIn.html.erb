<div class="container">

<div>
  <h1 class="col-xs-12">
    Welcome back
    <%= current_user.first_name.capitalize %>
    <a id="editUser" class="btn btn-success" href="/users/<%= current_user.id %>/edit" target="_self">Edit info</a>

  </h1>

  <div class="col-xs-12">
    <% if current_user.account == 'restaurant' %>
        <%= render 'restaurant' %>
    <% end %>

    <% if current_user.account == 'evaluator' %>
        <%= render 'evaluator/home' %>
    <% end %>
  </div>
</div>
</div>


