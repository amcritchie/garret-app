<div class="container">
  <div id="metaInfo" data-restaurant-id="<%= @restaurant.id %>">
    <h1>
      <%= @restaurant.name.capitalize %> Evaluations
    </h1>
  </div>

  <table id="evaluationsTable" class="table table-striped" data-restaurant-id="<%= @restaurant.id %>">
    <thead>
    <tr class="tableKeys">
      <th>When</th>
      <th>Action Plan</th>
      <th>Application ID</th>
      <th>Total</th>
      <% @departments.each do |department| %>
          <th class="department-name"><%= department.name %></th>
      <% end %>
    </tr>
    </thead>
    <tbody class="tableBody">

    </tbody>
  </table>
</div>